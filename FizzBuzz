var n;
for (n=0; n<=15; n++){
  var m5=n%5;
  var m3=n%3;
  if(n>0){
    if(m3===0 && m5 !== 0){
      console.log(n+' Fizz');
    }else if(m3!==0 && m5===0){
      console.log(n+' Buzz');
    }else if(m3===0 && m5===0){
      console.log(n+' FIZZBUZZ');
    }else{
      console.log(n);
    }
  }else{
    console.log(n);
  }
}
